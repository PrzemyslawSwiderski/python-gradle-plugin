{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:monorepos",
    "group:recommended",
    "mergeConfidence:age-confidence-badges",
    "replacements:all",
    "workarounds:all",
    ":automergeAll",
    ":automergeRequireAllStatusChecks"
  ],
  "github-actions": {
    "enabled": false
  },
  "ignorePaths": [
    "examples/**"
  ]
}